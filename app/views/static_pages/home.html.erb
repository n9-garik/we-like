<% provide(:title, 'Home') %>
<% if user_signed_in? %>	
<div class="row">
	<aside class="span4">
		<section>
			<%= render 'shared/user_info' %>
		</section>
		<section>
			<%= render 'shared/stats'%>
		</section>
		<section>
			<%= render 'shared/wish_form' %>
		</section>
	</aside>
	<div class="span8">
		<h3>Лента желаний</h3>
		<%= render 'shared/feed' %>
	</div>
	<% else %>
	<%= render 'shared/unreghome'%>

<% end %>
</div>
